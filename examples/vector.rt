func vector(x,y){
    func object(message){
        func add(x,y){
            return x+y;
        }

        if(message == "x") return x;
        if(message == "y") return y;
        if(message == "add") return add;
        print "unknown message";
    }

    return object;
}

var a = vector(1,2);
var b = vector(3,4);
var c = a("add")(b("x"),a("y"));

print c;
